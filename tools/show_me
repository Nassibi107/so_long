#include "tools.h"

void	show_me(char **map)
{
	int	y;
	int	x;

	y = 0;
	while(map[y])
	{
		x = 0;
		while (map[y][x])
		{

			printf("%c",map[y][x]);
			x++;
		}
		printf("\n");
		y++;
	}
}
